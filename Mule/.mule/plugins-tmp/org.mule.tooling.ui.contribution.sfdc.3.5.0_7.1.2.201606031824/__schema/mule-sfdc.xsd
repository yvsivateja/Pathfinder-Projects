          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="integerType" use="optional" default="0" name="connectionTimeout">
            <xs:annotation>
              <xs:documentation>Specifies the amount of time, in milliseconds, that the consumer will attempt to establish a connection before it times out. Default value is 0, which means infinite.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="proxyHost">
            <xs:annotation>
              <xs:documentation>Hostname of the proxy. If this property is not set then no proxy will be used, otherwise a proxy will be used, but a proxy host must be specified.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="proxyPort">
            <xs:annotation>
              <xs:documentation>Port of the proxy. If host is set then this property must be set and cannot be a negative number.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="proxyUsername">
            <xs:annotation>
              <xs:documentation>Username used to authenticate against the proxy. If this property is not set then no authentication is going to be used against the proxy, otherwise this value must be specified.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="proxyPassword">
            <xs:annotation>
              <xs:documentation>Password used to authenticate against the proxy.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="timeObjectStore-ref">
            <xs:annotation>
              <xs:documentation>An ObjectStore instance to use in &lt;a href="#_get_updated_objects"&gt;SalesforceConnector#getUpdatedObjects&lt;/a&gt;</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="integerType" use="optional" default="5" name="batchSobjectMaxDepth">
            <xs:annotation>
              <xs:documentation>Creating a batch will create SObjects using this value for the MAX_DEPTH check.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="required" name="consumerKey">
            <xs:annotation>
              <xs:documentation>Consumer key for Salesforce connected app</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="required" name="keyStore">
            <xs:annotation>
              <xs:documentation>Path to key store used to sign data during authentication</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="required" name="storePassword">
            <xs:annotation>
              <xs:documentation>Password of key store</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstract-extension" name="config-with-oauth">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractExtensionType">
          <xs:annotation>
            <xs:documentation>Salesforce connector configuration that uses the OAuth2 dance for establishing a connection with the Salesforce system.</xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="1" name="apex-class-names">
              <xs:annotation>
                <xs:documentation></xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" maxOccurs="unbounded" name="apex-class-name">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" use="optional" name="value-ref"></xs:attribute>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
                <xs:attribute type="xs:string" use="optional" name="ref">
                  <xs:annotation>
                    <xs:documentation>The reference object for this parameter</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="1" maxOccurs="1" name="oauth-callback-config">
              <xs:annotation>
                <xs:documentation>Config for http callbacks.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:attribute type="xs:string" use="required" name="localPort"></xs:attribute>
                    <xs:attribute type="xs:string" use="optional" name="remotePort"></xs:attribute>
                    <xs:attribute type="xs:string" use="required" name="domain"></xs:attribute>
                    <xs:attribute type="xs:boolean" use="optional" default="false" name="async"></xs:attribute>
                    <xs:attribute type="xs:string" use="required" name="path"></xs:attribute>
                    <xs:attribute type="xs:string" use="optional" name="connector-ref"></xs:attribute>
                    <xs:attribute type="xs:string" use="optional" name="defaultAccessTokenId">
                      <xs:annotation>
                        <xs:documentation>A Mule Expression to use as access token id</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="oauth-store-config">
              <xs:annotation>
                <xs:documentation>Configuration element for storage of access tokens</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:attribute type="xs:string" use="required" name="objectStore-ref"></xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute type="xs:string" use="required" name="name">
            <xs:annotation>
              <xs:documentation>Give a name to this configuration so it can be later referenced by config-ref.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="required" name="consumerKey">
            <xs:annotation>
              <xs:documentation>Your application's client identifier (consumer key in Remote Access Detail).</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="required" name="consumerSecret">
            <xs:annotation>
              <xs:documentation>Your application's client secret (consumer secret in Remote Access Detail).</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="doubleType" use="optional" default="36.0" name="apiVersion">
            <xs:annotation>
              <xs:documentation>Salesforce API version. The default value is the latest version supported by the connector.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="clientId">
            <xs:annotation>
              <xs:documentation>Client ID for partners</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="assignmentRuleId">
            <xs:annotation>
              <xs:documentation>The ID of a specific assignment rule to run for the Case or Lead. The assignment rule is either set to active or inactive. The ID can be retrieved by querying the AssignmentRule
object. If specified, do not specify useDefaultRule. This element is ignored for accounts, because all territory assignment rules are applied.
&lt;p/&gt;
If the value is not in correct ID format (15-character or 18-character Salesforce ID), the call fails and a MALFORMED_ID exception is returned. Default value is false.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="booleanType" use="optional" default="false" name="useDefaultRule">
            <xs:annotation>
              <xs:documentation>If true, the default (active) assignment rule for a Case or Lead is used. If specified, do not specify an assignmentRuleId. If true for an Account, all
territory assignment rules are applied, and if false, no territory assignment rules are applied. Default value is false.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="booleanType" use="optional" default="false" name="allowFieldTruncationSupport">
            <xs:annotation>
              <xs:documentation>If true, truncates field values that are too long, which is the behavior in API versions 14.0 and earlier.
&lt;p/&gt;
Default is false: no change in behavior. If a string or text area value is too large, the operation fails and the fault code STRING_TOO_LONG is returned.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="booleanType" use="optional" default="false" name="fetchAllApexSoapMetadata">
            <xs:annotation>
              <xs:documentation>If checked then Datasense is performed for all Apex classes in the organization, otherwise it is performed only for the classes in Apex class names. If the organization
contains a lot of Apex classes this might cause ConnectionTimeout during Datasense. Default value is false.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="booleanType" use="optional" default="false" name="fetchAllApexRestMetadata">
            <xs:annotation>
              <xs:documentation>If checked then Datasense will be performed for all Apex classes in the organization else Datasense will be performed only for the classes in Apex class names. If the organization
contains many Apex classes this could cause ConnectionTimeout during Datasense. Default value is false.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="integerType" use="optional" default="0" name="readTimeout">
            <xs:annotation>
              <xs:documentation>Specifies the amount of time, in milliseconds, that the consumer will wait for a response before it times out. Default value is 0, which means infinite.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="integerType" use="optional" default="0" name="connectionTimeout">
            <xs:annotation>
              <xs:documentation>Specifies the amount of time, in milliseconds, that the consumer will attempt to establish a connection before it times out. Default value is 0, which means infinite.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="proxyHost">
            <xs:annotation>
              <xs:documentation>Hostname of the proxy. If this property is not set then no proxy will be used, otherwise a proxy will be used, but a proxy host must be specified.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="proxyPort">
            <xs:annotation>
              <xs:documentation>Port of the proxy. If host is set then this property must be set and cannot be a negative number.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="proxyUsername">
            <xs:annotation>
              <xs:documentation>Username used to authenticate against the proxy. If this property is not set then no authentication is going to be used against the proxy, otherwise this value must be specified.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="proxyPassword">
            <xs:annotation>
              <xs:documentation>Password used to authenticate against the proxy.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="timeObjectStore-ref">
            <xs:annotation>
              <xs:documentation>An ObjectStore instance to use in &lt;a href="#_get_updated_objects"&gt;SalesforceConnector#getUpdatedObjects&lt;/a&gt;</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="integerType" use="optional" default="5" name="batchSobjectMaxDepth">
            <xs:annotation>
              <xs:documentation>Creating a batch will create SObjects using this value for the MAX_DEPTH check.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" default="https://login.salesforce.com/services/oauth2/token" name="accessTokenUrl">
            <xs:annotation>
              <xs:documentation>The URL defined by the Service Provider to obtain an access token</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="OnNoTokenPolicyEnumType" use="optional" default="EXCEPTION" name="onNoToken">
            <xs:annotation>
              <xs:documentation>The URL defined by the Service Provider to obtain an access token</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" default="https://login.salesforce.com/services/oauth2/authorize" name="authorizationUrl">
            <xs:annotation>
              <xs:documentation>The URL defined by the Service Provider where the resource owner will be redirected to grant authorization to the connector</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/sfdc" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="AuthorizeType" substitutionGroup="mule:abstract-message-processor" name="authorize">
    <xs:annotation>
      <xs:documentation>Starts OAuth authorization process. It must be called from a flow with an http:inbound-endpoint.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/sfdc" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="UnauthorizeType" substitutionGroup="mule:abstract-message-processor" name="unauthorize">
    <xs:annotation>
      <xs:documentation>Unauthorizes the connector, forcing to re-use authorize again before accessing any protected message processor.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="AuthorizeType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="state">
          <xs:annotation>
            <xs:documentation>Any value that you wish to be sent with the callback</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="SalesforceOAuthDisplayEnumType" use="required" name="display">
          <xs:annotation>
            <xs:documentation>Tailors the login page to the user's device type.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="SalesforceOAuthImmediateEnumType" use="optional" default="FALSE" name="immediate">
          <xs:annotation>
            <xs:documentation>Avoid interacting with the user.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="SalesforceOAuthPromptEnumType" use="optional" name="prompt">
          <xs:annotation>
            <xs:documentation>Specifies how the authorization server prompts the user for reauthentication and reapproval</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="accessTokenUrl">
          <xs:annotation>
            <xs:documentation>The URL defined by the Service Provider to obtain an access token</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="accessTokenId">
          <xs:annotation>
            <xs:documentation>The Id with which the obtained access token will be stored. If not provided, then it will be the config name</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="authorizationUrl">
          <xs:annotation>
            <xs:documentation>The URL defined by the Service Provider where the resource owner will be redirected to grant authorization to the connector</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="UnauthorizeType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="optional" name="accessTokenId">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/sfdc" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="CreateType" substitutionGroup="mule:abstract-message-processor" name="create">
    <xs:annotation>
      <xs:documentation>Adds one or more new records to your organization's data.

&lt;p class="caution"&gt;
IMPORTANT: When you map your objects to the input of this message processor keep in mind that they need to match the expected type of the object at Salesforce.

Take the CloseDate of an Opportunity as an example, if you set that field to a string of value "2011-12-13" it will be sent to Salesforce as a string and operation will be
rejected on the basis that CloseDate is not of the expected type.

The proper way to actually map it is to generate a Java Date object, you can do so using Groovy expression evaluator as &lt;i&gt;#[groovy:Date.parse("yyyy-MM-dd",
"2011-12-13")]&lt;/i&gt;.
&lt;/p&gt;</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="CreateType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="objects">
            <xs:annotation>
              <xs:documentation>An array of one or more sObject objects.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="object">
                  <xs:complexType>
                    <xs:choice>
                      <xs:sequence>
                        <xs:element minOccurs="0" maxOccurs="unbounded" name="inner-object">
                          <xs:complexType>
                            <xs:simpleContent>
                              <xs:extension base="xs:string">
                                <xs:attribute type="xs:string" use="optional" name="value-ref"></xs:attribute>
                                <xs:attribute type="xs:string" name="key"></xs:attribute>
                              </xs:extension>
                            </xs:simpleContent>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                      <xs:sequence>
                        <xs:any minOccurs="0" maxOccurs="unbounded" processContents="lax"></xs:any>
                      </xs:sequence>
                    </xs:choice>
                    <xs:attribute type="xs:string" use="optional" name="ref">
                      <xs:annotation>
                        <xs:documentation>The reference object for this parameter</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
              <xs:attribute type="xs:string" use="optional" name="ref">
                <xs:annotation>
                  <xs:documentation>The reference object for this parameter</xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="headers">
            <xs:annotation>
              <xs:documentation>Salesforce Headers &lt;a href="http://www.salesforce.com/us/developer/docs/api/Content/soap_headers.htm"&gt;More Info&lt;/a&gt;</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:choice>
                <xs:sequence>
                  <xs:element minOccurs="0" maxOccurs="unbounded" name="header">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" use="optional" name="value-ref"></xs:attribute>
                          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="SalesforceHeaderEnumType" name="key"></xs:attribute>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
                <xs:sequence>
                  <xs:any minOccurs="0" maxOccurs="unbounded" processContents="lax"></xs:any>
                </xs:sequence>
              </xs:choice>
              <xs:attribute type="xs:string" use="optional" name="ref">
                <xs:annotation>
                  <xs:documentation>The reference object for this parameter</xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="type">
          <xs:annotation>
            <xs:documentation>Type of object to create</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="accessTokenId">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/sfdc" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Create-jobType" substitutionGroup="mule:abstract-message-processor" name="create-job">
    <xs:annotation>
      <xs:documentation>Creates a Job in order to perform one or more batches through Bulk API Operations.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Create-jobType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="OperationEnumEnumType" use="required" name="operation">
          <xs:annotation>
            <xs:documentation>The com.sforce.async.OperationEnum that will be executed by the job.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="type">
          <xs:annotation>
            <xs:documentation>The type of Salesforce object that the job will process.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="externalIdFieldName">
          <xs:annotation>
            <xs:documentation>Contains the name of the field on this object with the external ID field attribute for custom objects or the idLookup field property for standard objects (only required for Upsert Operations).</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="ContentTypeEnumType" use="optional" name="contentType">
          <xs:annotation>
            <xs:documentation>The Content Type for this Job results. When specifying a content type different from XML for a query type use { #queryResultStream(com.sforce.async.BatchInfo)} batchResultStream} method to retrieve results.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sfdc" type="ConcurrencyModeEnumType" use="optional" name="concurrencyMode">
          <xs:annotation>
            <xs:documentation>The concurrency mode of the job, either Parallel or Serial.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="accessTokenId">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/sfdc" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Close-jobType" substitutionGroup="mule:abstract-message-processor" name="close-job">
    <xs:annotation>
      <xs:documentation>Closes an open Job given its ID.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Close-jobType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="jobId">
          <xs:annotation>
            <xs:documentation>The Job ID identifying the Job to be closed.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="accessTokenId">
          <xs:annotation>
            <xs:documentation>The id of the access token that will be used to authenticate the call</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
